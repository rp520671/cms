{
  "version": 2,
  "builds": [
    {
      "src": "/app.js",
      "use": "@vercel/node"
    }
  ],

  "routes": [
    {
      "src": "/adminroutes",
      "dest": "/adminroutes.js"
    },
    {
      "src": "/defaultroutes",
      "dest": "/defaultroutes.js"
    },
    {
      "src": "/(.*)",
      "dest": "/app.js"
    }
  ]
}
