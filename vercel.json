{
  "version": 2,
  "builds": [
    {
      "src": "../app.js",
      "use": "@vercel/node"
    }
  ],

  "routes": [
    {
      "src": "/adminroutes",
      "dest": "../routes/adminroutes.js"
    },
    {
      "src": "/defaultroutes",
      "dest": "../routes/defaultroutes.js"
    },
    {
      "src": "/(.*)",
      "dest": "../app.js"
    }
  ]
}
